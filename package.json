    "build:bbc": "PLATFORM=bbc vue-cli-service build",
    "build:b2c": "PLATFORM=b2c vue-cli-service build",
    "dev:bbc": "PLATFORM=bbc vue-cli-service serve",
    "dev:b2c": "PLATFORM=b2c vue-cli-service serve",
    "sh": "node ./cli.js",
